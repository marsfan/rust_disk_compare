{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "rust_disk_compare Debug Windows",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/disk_compare.exe",
            "args": [
                "${input:first_dir}",
                "${input:second_dir}"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "rust: cargo build"
        }
    ],
    "inputs": [
        {
            "id": "first_dir",
            "type": "promptString",
            "default": "",
            "description": "First argument to pass to tool."
        },
        {
            "id": "second_dir",
            "type": "promptString",
            "default": "",
            "description": "Second argument to pass to tool."
        }
    ]
}